{
    "manifest_version": 2,
    "name": "Beautiful GitHub",
    "version": "1.0.4",
    "description": "Beautiful new colors for GitHub contributions graph picked randomly.",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "author": "Vatsal Joshi",
    "homepage_url": "https://vatz88.in/Beautiful-GitHub/",
    "content_scripts": [{
        "matches": ["https://github.com/*"],
        "js": ["jquery.slim.min.js", "script.js"],
        "run_at": "document_end"
    }]
}
